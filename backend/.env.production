# Production Environment Variables for Backend
NODE_ENV=production
PORT=10000

# MongoDB Atlas Connection (Replace with your actual connection string)
MONGO_URI=mongodb+srv://ecometer-user:your-password@cluster0.mongodb.net/ecometer?retryWrites=true&w=majority

# ML Service URL (Your deployed Render ML service URL)
ML_SERVICE_URL=https://ecometer-ml-service.onrender.com

# Frontend URL (Replace with your actual Vercel frontend URL)
FRONTEND_URL=https://ecometer-frontend.vercel.app

# CORS Origin (Same as frontend URL)
CORS_ORIGIN=https://ecometer-frontend.vercel.app